{
  "name": "freight-delay-notification",
  "version": "1.0.0",
  "description": "Real-time freight delay notification system with Temporal workflow orchestration",
  "repository": {
    "type": "git",
    "url": "https://github.com/red-game-dev/the-freight-delay-notification.git"
  },
  "bugs": {
    "url": "https://github.com/red-game-dev/the-freight-delay-notification/issues"
  },
  "homepage": "https://github.com/red-game-dev/the-freight-delay-notification#readme",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "biome check",
    "format": "biome format --write",
    "type:check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:test": "test-storybook",
    "db:start": "npx supabase@latest start",
    "db:stop": "npx supabase@latest stop",
    "db:reset": "npx supabase@latest db reset",
    "db:migrate": "tsx scripts/migrate-database.ts",
    "db:check": "tsx scripts/check-remote-schema.ts",
    "db:check:migrations": "tsx scripts/check-migrations.ts",
    "db:clear": "tsx scripts/clear-all-data.ts",
    "db:status": "npx supabase@latest status",
    "db:login": "npx supabase@latest login",
    "temporal": "bash scripts/temporal.sh",
    "temporal:stop": "docker stop temporal-dev-server && docker rm temporal-dev-server",
    "temporal:logs": "docker logs -f temporal-dev-server",
    "temporal:test": "tsx scripts/test-temporal-connection.ts",
    "temporal:worker": "tsx src/workflows/worker.ts",
    "temporal:terminate": "tsx scripts/terminate-workflow.ts",
    "test:workflow": "tsx scripts/test-complete-workflow.ts",
    "test:workflow:custom": "tsx scripts/test-complete-workflow.ts -- --threshold 5",
    "test:route:la": "tsx scripts/test-complete-workflow.ts -- --origin \"Downtown Los Angeles, CA\" --destination \"LAX Airport, CA\" --threshold 30",
    "test:route:ny": "tsx scripts/test-complete-workflow.ts -- --origin \"Times Square, Manhattan, NY\" --destination \"JFK Airport, Queens, NY\" --threshold 30",
    "test:route:sf": "tsx scripts/test-complete-workflow.ts -- --origin \"Downtown San Francisco, CA\" --destination \"San Jose, CA\" --threshold 45",
    "app:dev": "concurrently \"pnpm run dev\" \"pnpm run temporal:worker\" \"pnpm run cron:dev\" \"pnpm run monitor:system\" \"pnpm run health:check\"",
    "cron:dev": "tsx scripts/run-traffic-cron.ts",
    "cron:prod": "tsx scripts/run-traffic-cron.ts --prod",
    "cron:test": "curl -X GET http://localhost:3000/api/cron/check-traffic -H \"Authorization: Bearer ${CRON_SECRET}\"",
    "monitor:system": "tsx scripts/monitor-system.ts",
    "health:check": "tsx scripts/health-check.ts"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.4.2",
    "@mapbox/mapbox-sdk": "^0.16.2",
    "@react-google-maps/api": "^2.20.7",
    "@sendgrid/mail": "^8.1.6",
    "@supabase/supabase-js": "^2.58.0",
    "@tanstack/react-query": "^5.90.2",
    "@temporalio/activity": "^1.13.0",
    "@temporalio/client": "^1.13.0",
    "@temporalio/worker": "^1.13.0",
    "@temporalio/workflow": "^1.13.0",
    "clsx": "^2.1.1",
    "immer": "^10.1.3",
    "lucide-react": "^0.395.0",
    "nanoid": "^5.1.6",
    "next": "15.5.4",
    "next-themes": "^0.4.6",
    "openai": "^4.52.0",
    "pino": "^9.2.0",
    "pino-pretty": "^11.2.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.52.0",
    "tailwind-merge": "^2.3.0",
    "twilio": "^5.2.0",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@storybook/addon-essentials": "^8.1.0",
    "@storybook/addon-interactions": "^8.1.0",
    "@storybook/addon-links": "^8.1.0",
    "@storybook/blocks": "^8.1.0",
    "@storybook/nextjs": "^8.1.0",
    "@storybook/react": "^8.1.0",
    "@storybook/test": "^8.1.0",
    "@storybook/test-runner": "^0.18.0",
    "@tailwindcss/postcss": "^4",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.18",
    "@types/react": "^19.1.15",
    "@types/react-dom": "^19.1.9",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "dotenv": "^17.2.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.6",
    "storybook": "^8.1.0",
    "supabase": "^1.226.4",
    "tailwindcss": "^4.1.13",
    "ts-jest": "^29.4.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}
