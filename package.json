{
  "name": "freight-delay-notification",
  "version": "1.0.0",
  "description": "Real-time freight delay notification system with Temporal workflow orchestration",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "biome check",
    "format": "biome format --write",
    "type:check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:test": "test-storybook",
    "db:migrate": "pnpm supabase migration up",
    "db:seed": "tsx scripts/seed.ts",
    "db:reset": "pnpm supabase db reset",
    "temporal": "bash scripts/temporal.sh",
    "temporal:stop": "docker stop temporal-dev-server && docker rm temporal-dev-server",
    "temporal:logs": "docker logs -f temporal-dev-server",
    "temporal:test": "tsx scripts/test-temporal-connection.ts",
    "temporal:worker": "tsx src/workflows/worker.ts",
    "temporal:dev": "concurrently \"pnpm run dev\" \"pnpm run temporal:worker\""
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.4.2",
    "@mapbox/mapbox-sdk": "^0.16.2",
    "@sendgrid/mail": "^8.1.6",
    "@supabase/supabase-js": "^2.58.0",
    "@tanstack/react-query": "^5.90.2",
    "@temporalio/activity": "^1.13.0",
    "@temporalio/client": "^1.13.0",
    "@temporalio/worker": "^1.13.0",
    "@temporalio/workflow": "^1.13.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.395.0",
    "next": "15.5.4",
    "openai": "^4.52.0",
    "pino": "^9.2.0",
    "pino-pretty": "^11.2.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.52.0",
    "tailwind-merge": "^2.3.0",
    "twilio": "^5.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@storybook/addon-essentials": "^8.1.0",
    "@storybook/addon-interactions": "^8.1.0",
    "@storybook/addon-links": "^8.1.0",
    "@storybook/blocks": "^8.1.0",
    "@storybook/nextjs": "^8.1.0",
    "@storybook/react": "^8.1.0",
    "@storybook/test": "^8.1.0",
    "@storybook/test-runner": "^0.18.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.18",
    "@types/react": "^19.1.15",
    "@types/react-dom": "^19.1.9",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.6",
    "storybook": "^8.1.0",
    "supabase": "^1.226.4",
    "tailwindcss": "^4.1.13",
    "ts-jest": "^29.4.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}